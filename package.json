{
  "name": "deside-hackathon",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo workspace for Deside Hackathon backend and frontend apps.",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "demo": "node scripts/run-demo.mjs",
    "dev:backend": "npm run dev --workspace backend",
    "dev:frontend": "npm run dev --workspace frontend",
    "build": "npm run build --workspace frontend",
    "lint": "npm run lint --workspace frontend",
    "test": "npm run test --workspace frontend",
    "dev:parallel": "npm run dev",
    "style:analyze": "cd ai-style-agent && npm run analyze",
    "style:design": "node ai-style-agent/src/index.js design",
    "style:preview": "node ai-style-agent/src/index.js preview",
    "style:apply": "node ai-style-agent/src/index.js apply",
    "style:cancel": "node ai-style-agent/src/index.js cancel",
    "style:migrate": "node ai-style-agent/src/index.js migrate",
    "style:export": "node ai-style-agent/src/index.js export",
    "style:export-visual": "node ai-style-agent/src/index.js export-visual",
    "style:validate": "node ai-style-agent/src/index.js validate",
    "style:validate:local": "node ai-style-agent/src/index.js validate --local",
    "style:watch": "cd ai-style-agent && npm run watch",
    "style:watch:local": "cd ai-style-agent && npm run watch:local",
    "style:report": "node ai-style-agent/src/index.js report",
    "style:iterate": "node ai-style-agent/src/index.js iterate",
    "style:insights": "node ai-style-agent/src/index.js insights",
    "style:backups": "node ai-style-agent/src/index.js backups",
    "style:restore": "node ai-style-agent/src/index.js restore",
    "style:delete-backup": "node ai-style-agent/src/index.js delete-backup",
    "style:consolidate": "node ai-style-agent/src/index.js consolidate",
    "token:add": "node ai-token-agent/src/index.js add",
    "token:discover": "node ai-token-agent/src/index.js discover",
    "token:list": "node ai-token-agent/src/index.js list",
    "token:remove": "node ai-token-agent/src/index.js remove",
    "token:insights": "node ai-token-agent/src/index.js insights",
    "token:cleanup": "node ai-token-agent/src/index.js cleanup",
    "token:update-prices": "node ai-token-agent/src/index.js update-prices",
    "token:watch": "node ai-token-agent/src/index.js watch",
    "token": "npm run token:add",
    "stats:api": "DATA_MODE=memory ALLOWED_ORIGINS=http://localhost:3002 PORT=3001 npm run dev --workspace backend",
    "stats:panel": "bash -lc 'set -a; [ -f frontend/.env ] && . frontend/.env; VITE_API_BASE_URL=${VITE_API_BASE_URL:-http://localhost:3001} VITE_ADMIN_JWT=\"$VITE_ADMIN_JWT\" npm --prefix admin-panel run dev -- --host --port 3002'",
    "stats": "concurrently \"npm:stats:api\" \"npm:stats:panel\"",
    "stats:setup": "npm --prefix admin-panel install",
    "telegram:bot": "node backend/src/modules/telegram-bot/startBot.js",
    "telegram:dev": "nodemon backend/src/modules/telegram-bot/startBot.js",
    "telegram:test": "node scripts/test-telegram-bot.mjs",
    "telegram:setup": "./scripts/quick-telegram-setup.sh"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/web3.js": "^1.98.4",
    "@wallet-standard/features": "^1.1.0",
    "@walletconnect/utils": "^2.21.6",
    "axios": "^1.7.9",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.1.2",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.3",
    "dotenv": "^16.6.1",
    "envalid": "^8.1.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.475.0",
    "mitt": "^3.0.1",
    "mongoose": "^8.9.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.28.1",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stream-browserify": "^3.0.0",
    "telegraf": "^4.16.3",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.1.9",
    "typescript-eslint": "^8.40.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
