# RTC-first configuration
VITE_USE_WEBRTC_FOR_TEXT=true
VITE_FORCE_RELAY=false

# Timeouts y TTL recomendados (alineados con backend)
VITE_PRESENCE_TTL_MS=45000
VITE_RTC_OPEN_TIMEOUT_MS=2300
VITE_HEARTBEAT_INTERVAL_MS=35000

# Relay endpoints (ya configurados)
VITE_RELAY_ENQUEUE_PATH=/api/v1/relay/enqueue
VITE_RELAY_FETCH_PATH=/api/v1/relay/fetch
VITE_RELAY_ACK_PATH=/api/v1/relay/ack

# ICE endpoint
VITE_RTC_ICE_PATH=/api/v1/rtc/ice

# Feature flags endpoint  
VITE_USER_FLAGS_PATH=/api/v1/flags/me

# Backend local
VITE_API_BASE_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001

# Ejemplo para OnRender
# VITE_API_BASE_URL=https://tu-app.onrender.com
# VITE_WS_URL=wss://tu-app.onrender.com

VITE_FEATURE_SWAP=false
VITE_FEATURE_IDENTITY=false
VITE_DEBUG_WALLETS=false
VITE_FEATURE_AGREEMENT_VERIFY=false
VITE_FEATURE_AGREEMENT_SETTLEMENT=false
VITE_PAYMENT_INLINE_EXEC=true
VITE_ENABLE_BLINK_BUY_MOCK=false
VITE_DIALECT_API_BASE_URL=https://api.dial.to/v1
VITE_BLINK_CLIENT_KEY=

# E2EE estático (32 bytes base64 compartidos con /.env)
VITE_MESSAGING_E2EE_SHARED_KEY_B64=YOUR_SHARED_32B_BASE64
VITE_DEV_SERVER_PORT=3000

# Wallet UI mode: 'modal' (recommended) or 'side'
VITE_WALLET_PANEL_MODE=modal

# Solana chain
VITE_SOLANA_CHAIN=devnet

# Jupiter Buy (token mints) — fill with correct mainnet mints
# BONK
VITE_MINT_BONK=
# PENGU
VITE_MINT_PENGU=
# JUP
VITE_MINT_JUP=
# WIF
VITE_MINT_WIF=
